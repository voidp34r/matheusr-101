{
  "version": "0.1.0",
  "configurations": [

      {
          "name": "Launch localhost",
          "type": "chrome",
          "request": "launch",
          "url": "http://localhost:4200/",
          "webRoot": "${workspaceFolder}"
      },
      {
          "name": "Launch index.html (disable sourcemaps)",
          "type": "chrome",
          "request": "launch",
          "sourceMaps": false,
          "file": "${workspaceFolder}/"
      },
  ]
}
